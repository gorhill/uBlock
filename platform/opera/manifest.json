{
  "author": "All uBlock Origin contributors",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "16": "img/icon_16.png",
      "32": "img/icon_32.png"
    },
    "default_popup": "popup.html",
    "default_title": "uBlock Origin"
  },
  "commands": {
    "launch-element-picker": {
      "description": "__MSG_popupTipPicker__"
    },
    "launch-element-zapper": {
      "description": "__MSG_popupTipZapper__"
    },
    "launch-logger": {
      "description": "__MSG_popupTipLog__"
    }
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "js/vapi.js",
        "js/vapi-client.js",
        "js/contentscript.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    },
    {
      "all_frames": false,
      "js": [
        "js/scriptlets/subscriber.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "default_locale": "en",
  "description": "__MSG_extShortDesc__",
  "icons": {
    "16": "img/icon_16.png",
    "32": "img/icon_32.png",
    "64": "img/icon_64.png",
    "128": "img/icon_128.png"
  },
  "incognito": "split",
  "manifest_version": 2,
  "minimum_opera_version": "38.0",
  "name": "uBlock Origin",
  "optional_permissions": [
    "file:///*"
  ],
  "options_page": "dashboard.html",
  "permissions": [
    "contextMenus",
    "privacy",
    "storage",
    "tabs",
    "unlimitedStorage",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "short_name": "uBlockâ‚€",
  "sidebar_action": {
    "default_icon": {
      "16": "img/icon_16.png",
      "32": "img/icon_32.png"
    },
    "default_panel": "logger-ui.html",
    "default_title": "__MSG_statsPageName__"
  },
  "version": "1.14.23.17",
  "web_accessible_resources": [
    "/web_accessible_resources/*"
  ]
}
