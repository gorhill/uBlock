{
  "manifest_version": 2,
  "name": "AdNauseam",
  "version": "{version}",
  "default_locale": "en",
  "description": "__MSG_extShortDesc__",
  "icons": {
    "16": "img/icon_16.png",
    "128": "img/icon_128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "img/icon-16.png",
      "32": "img/icon-32.png"
    },
    "default_title": "AdNauseam",
    "default_popup": "menu.html"
  },
  "author": "Daniel C. Howe",
  "background": {
    "page": "background.html"
  },
  "commands": {
    "launch-element-zapper": {
        "description": "__MSG_popupTipZapper__"
    },
    "launch-element-picker": {
        "description": "__MSG_popupTipPicker__"
    },
    "launch-logger": {
        "description": "__MSG_popupTipLog__"
    },
    "open-dashboard": {
      "description": "__MSG_popupTipDashboard__"
    },
    "relax-blocking-mode": {
      "description": "__MSG_relaxBlockingMode__"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "js/vapi.js",
        "js/vapi-client.js",
        "js/contentscript.js",
        "js/adn/textads.js", 
        "js/adn/parser.js"
      ],
      "all_frames": true,
      "match_about_blank": true,
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://easylist.to/*",
        "https://*.fanboy.co.nz/*",
        "https://filterlists.com/*",
        "https://forums.lanik.us/*",
        "https://github.com/*",
        "https://*.github.io/*"
      ],
      "js": [
        "/js/scriptlets/subscriber.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "minimum_opera_version": "42.0",
  "incognito": "spanning",
  "options_page": "dashboard.html",
  "permissions": [
    "contextMenus",
    "privacy",
    "storage",
    "tabs",
    "unlimitedStorage",
    "webNavigation",
    "webRequestBlocking",
    "webRequest",
    "downloads",
    "<all_urls>",
    "management"
  ],
  "short_name": "AdNauseam",
  "storage": {
    "managed_schema": "managed_storage.json"
  },
  "web_accessible_resources": [
    "/web_accessible_resources/*"
  ]
}
